{
  "name": "neilmaggs.co.uk",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://neilmaggs.co.uk",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "gsap": "^3.3.3",
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/cli-service": "~5.0.8",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^13.2.0",
    "vue-template-compiler": "^2.6.11",
    "gh-pages": "^2.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "import/extensions": ["error", "always", { "vue": "never", "js": "never" }]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".vue"]
        }
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
